{
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "env": {
    "node": true,
    "browser": true
  },
  "parser": "@typescript-eslint/parser",
  "extends": [
    "airbnb",
    "airbnb-typescript"
  ],
  "rules": {
    "import/prefer-default-export": "off",
    "no-console": "off",
    "max-len": "off"
  }
}
